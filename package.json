    "@testing-library/user-event": "^12.1.10",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "4.x",
